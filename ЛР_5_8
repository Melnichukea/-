1) Сортировкап выбором
public class SelectionSort {
    
    public static void selectionSort(int[] arr) {
        int n = arr.length;
        
        for (int i = 0; i < n - 1; i++) {
            // Ищем минимальный элемент в оставшейся неотсортированной части
            int minIndex = i;
            for (int j = i + 1; j < n; j++) {
                if (arr[j] < arr[minIndex]) {
                    minIndex = j;
                }
            }
            
            // Меняем местами найденный минимальный элемент с первым элементом
            if (minIndex != i) {
                int temp = arr[i];
                arr[i] = arr[minIndex];
                arr[minIndex] = temp;
            }
        }
    }
    
    // Метод для вывода массива
    public static void printArray(int[] arr) {
        for (int value : arr) {
            System.out.print(value + " ");
        }
        System.out.println();
    }
    
    // Пример использования
    public static void main(String[] args) {
        int[] array = {64, 25, 12, 22, 11};
        
        System.out.println("Исходный массив:");
        printArray(array);
        
        selectionSort(array);
        
        System.out.println("Отсортированный массив:");
        printArray(array);
    }
}

 С++
#include <iostream>
#include <vector>

using namespace std;

void selectionSort(vector<int>& arr) {
    int n = arr.size();
    
    for (int i = 0; i < n - 1; i++) {
        // Ищем минимальный элемент в оставшейся неотсортированной части
        int minIndex = i;
        for (int j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIndex]) {
                minIndex = j;
            }
        }


        
        // Меняем местами найденный минимальный элемент с первым элементом
        if (minIndex != i) {
            swap(arr[i], arr[minIndex]);
        }
    }
}

// Функция для вывода массива
void printArray(const vector<int>& arr) {
    for (int value : arr) {
        cout << value << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    vector<int> array = {64, 25, 12, 22, 11};
    
    cout << "Исходный массив:" << endl;
    printArray(array);
    
    selectionSort(array);
    
    cout << "Отсортированный массив:" << endl;
    printArray(array);
    
    return 0;
}

2) Сортировка обменом (пузырёк)
public class BubbleSort {
    // Метод для сортировки массива пузырьком
    public static void bubbleSort(int[] arr) {
        int n = arr.length;
        boolean swapped;
        
        // Проходим по массиву n-1 раз
        for (int i = 0; i < n - 1; i++) {
            swapped = false;
            
            // Внутренний цикл: сравниваем соседние элементы
            for (int j = 0; j < n - i - 1; j++) {
                if (arr[j] > arr[j + 1]) {
                    // Меняем элементы местами
                    int temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                    swapped = true;
                }
            }
            
            // Если перестановок не было — массив уже отсортирован
            if (!swapped) break;
        }
    }
    
    // Метод для вывода массива
    public static void printArray(int[] arr) {
        for (int value : arr) {
            System.out.print(value + " ");
        }
        System.out.println();
    }
    
    // Пример использования
    public static void main(String[] args) {
        int[] array = {64, 34, 25, 12, 22, 11, 90};
        
        System.out.println("Исходный массив:");
        printArray(array);
        
        bubbleSort(array);
        
        System.out.println("Отсортированный массив:");
        printArray(array);
    }
}

С++
#include <iostream>
#include <vector>
using namespace std;

// Функция для сортировки массива пузырьком
void bubbleSort(vector<int>& arr) {
    int n = arr.size();
    bool swapped;
    
    // Проходим по массиву n-1 раз
    for (int i = 0; i < n - 1; i++) {
        swapped = false;
        
        // Внутренний цикл: сравниваем соседние элементы
        for (int j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                // Меняем элементы местами
                swap(arr[j], arr[j + 1]);
                swapped = true;
            }
        }
        
        // Если перестановок не было — массив уже отсортирован
        if (!swapped) break;
    }
}

// Функция для вывода массива
void printArray(const vector<int>& arr) {
    for (int value : arr) {
        cout << value << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    vector<int> array = {64, 34, 25, 12, 22, 11, 90};
    
    cout << "Исходный массив:" << endl;
    printArray(array);
    
    bubbleSort(array);
    
    cout << "Отсортированный массив:" << endl;
    printArray(array);
    
    return 0;
}

3) Сортировка вставками
public class InsertionSort {
    
    // Метод для сортировки массива
    public static void insertionSort(int[] arr) {
        int n = arr.length;
        
        // Проходим по всем элементам массива начиная со второго
        for (int i = 1; i < n; ++i) {
            int key = arr[i];  // Текущий элемент для вставки
            int j = i - 1;  // Индекс последнего элемента в отсортированной части
            
            // Перемещаем элементы больше key на одну позицию вперёд
            while (j >= 0 && arr[j] > key) {
                arr[j + 1] = arr[j];
                j = j - 1;
            }
            
            // Вставляем key на правильное место
            arr[j + 1] = key;
        }
    }
    
    // Метод для вывода массива
    public static void printArray(int[] arr) {
        for (int value : arr) {
            System.out.print(value + " ");
        }
        System.out.println();
    }
    
    // Пример использования
    public static void main(String[] args) {
        int[] array = {12, 11, 13, 5, 6};
        
        System.out.println("Исходный массив:");
        printArray(array);
        
        insertionSort(array);
        
        System.out.println("Отсортированный массив:");
        printArray(array);
    }
}

С++
#include <iostream>
using namespace std;

// Функция для сортировки массива
void insertionSort(int arr[], int n) {
    for (int i = 1; i < n; ++i) {
        int key = arr[i];  // Текущий элемент для вставки
        int j = i - 1;   // Индекс последнего элемента в отсортированной части
        
        // Перемещаем элементы больше key на одну позицию вперёд
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        
        // Вставляем key на правильное место
        arr[j + 1] = key;
    }
}

// Функция для вывода массива
void printArray(int arr[], int n) {
    for (int i = 0; i < n; ++i) {
        cout << arr[i] << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    int array[] = {12, 11, 13, 5, 6};
    int n = sizeof(array) / sizeof(array[0]);
    
    cout << "Исходный массив:" << endl;
    printArray(array, n);
    
    insertionSort(array, n);
    
    cout << "Отсортированный массив:" << endl;
    printArray(array, n);
    
    return 0;
}
4) Сортитровка слиянием
public class MergeSort {

    // Основной метод сортировки
    public static void mergeSort(int[] arr) {
        if (arr.length <= 1) {
            return; // Массив из 0 или 1 элемента уже отсортирован
        }
        
        int mid = arr.length / 2;
        int[] left = new int[mid];
        int[] right = new int[arr.length - mid];
        
        // Делим массив на две части
        System.arraycopy(arr, 0, left, 0, mid);
        System.arraycopy(arr, mid, right, 0, arr.length

С++
#include <iostream>
#include <vector>
using namespace std;

// Функция слияния двух отсортированных подмассивов
void merge(vector<int>& arr, int left, int mid, int right) {
    int n1 = mid - left + 1;  // Размер левого подмассива
    int n2 = right - mid;      // Размер правого подмассива

    // Создаем временные массивы для хранения подмассивов
    vector<int> leftArr(n1);
    vector<int> rightArr(n2);

    // Заполняем временные массивы
    for (int i = 0; i < n1; i++) {
        leftArr[i] = arr[left + i];
    }
    for (int j = 0; j < n2; j++) {
        rightArr[j] = arr[mid + 1 + j];
    }

    // Слияние подмассивов в исходный массив
    int i = 0, j = 0, k = left;
    while (i < n1 && j < n2) {
        if (leftArr[i] <= rightArr[j]) {
            arr[k] = leftArr[i];
            i++;
        } else {
            arr[k] = rightArr[j];
            j++;
        }
        k++;
    }

    // Добавляем оставшиеся элементы левого подмассива
    while (i < n1) {
        arr[k] = leftArr[i];
        i++;
        k++;
    }

    // Добавляем оставшиеся элементы правого подмассива
    while (j < n2) {
        arr[k] = rightArr[j];
        j++;
        k++;
    }
}

// Рекурсивная функция сортировки слиянием
void mergeSort(vector<int>& arr, int left, int right) {
    if (left < right) {
        // Находим середину массива
        int mid = left + (right - left) / 2;

        // Сортируем левую и правую половины
        mergeSort(arr, left, mid);
        mergeSort(arr, mid + 1, right);

        // Сливаем отсортированные половины
        merge(arr, left, mid, right);
    }
}

// Пример использования
int main() {
    vector<int> arr = {64, 34, 25, 12, 22, 11, 90};

    cout << "Исходный массив:" << endl;
    for (int num : arr) {
        cout

5) Сортировка Шелла
import java.util.Arrays;

public class ShellSort {
    // Метод для сортировки массива
    public void shellSort(int[] array) {
        int n = array.length;
        
        // Начинаем с интервала n/2 и уменьшаем его вдвое на каждом шаге
        for (int interval = n / 2; interval > 0; interval /= 2) {
            // Проходим по элементам с текущим интервалом
            for (int i = interval; i < n; i++) {
                int temp = array[i];  // Текущий элемент для вставки
                int j = i;
                
                // Перемещаем элементы, большие temp, на интервал вправо
                while (j >= interval && array[j - interval] > temp) {
                    array[j] = array[j - interval];
                    j -= interval;
                }
                
                // Вставляем temp на правильное место
                array[j] = temp;
            }
        }
    }

    // Пример использования
    public static void main(String[] args) {
        int[] data = {9, 8, 3, 7, 5, 6, 4, 1};
        ShellSort ss = new ShellSort();
        
        System.out.println("Исходный массив: " + Arrays.toString(data));
        ss.shellSort(data);
        System.out.println("Отсортированный массив: " + Arrays.toString(data));
    }
}

С++
#include <iostream>
#include <vector>

using namespace std;

// Функция для сортировки массива
void shellSort(vector<int>& array) {
    int n = array.size();
    
    // Начинаем с интервала n/2 и уменьшаем его вдвое на каждом шаге
    for (int interval = n / 2; interval > 0; interval /= 2) {
        // Проходим по элементам с текущим интервалом
        for (int i = interval; i < n; i++) {
            int temp = array[i];  // Текущий элемент для вставки
            int j = i;
            
            // Перемещаем элементы, большие temp, на интервал вправо
            while (j >= interval && array[j - interval] > temp) {
                array[j] = array[j - interval];
                j -= interval;
            }
            
            // Вставляем temp на правильное место
            array[j] = temp;
        }
    }
}

// Функция для вывода массива
void printArray(const vector<int>& array) {
    for (int element : array) {
        cout << element << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    vector<int> data = {9, 8, 3, 7, 5, 6, 4, 1};
    
    cout << "Исходный массив: ";
    printArray(data);
    
    shellSort(data);
    
    cout << "Отсортированный массив: ";
    printArray(data);
    
    return 0;
}

6) Быстрая сортировка
public class QuickSort {
    // Основной метод сортировки
    public static void quickSort(int[] arr, int low, int high) {
        if (low < high) {
            // Получаем индекс опорного элемента после partition
            int pivotIndex = partition(arr, low, high);
            
            // Рекурсивно сортируем левую и правую части
            quickSort(arr, low, pivotIndex - 1);
            quickSort(arr, pivotIndex + 1, high);
        }
    }
    
    // Метод для разделения массива и выбора опорного элемента
    private static int partition(int[] arr, int low, int high) {
        // Выбираем последний элемент как опорный
        int pivot = arr[high];
        int i = low - 1; // Индекс меньшего элемента
        
        for (int j = low; j < high; j++) {
            // Если текущий элемент меньше или равен опорному
            if (arr[j] <= pivot) {
                i++;
                swap(arr, i, j);
            }
        }
        
        // Перемещаем опорный элемент на правильное место
        swap(arr, i + 1, high);
        return i + 1;
    }
    
    // Метод для обмена элементов массива
    private static void swap(int[] arr, int i, int j) {
        int temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
    }
    
    // Пример использования
    public static void main(String[] args) {
        int[] array = {10, 7, 8, 9, 1, 5};
        int n = array.length;
        
        System.out.println("Исходный массив:");
        printArray(array);
        
        quickSort(array, 0, n - 1);
        
        System.out.println("Отсортированный массив:");
        printArray(array);
    }
    
    // Метод для вывода массива
    private static void printArray(int[] arr) {
        for (int value : arr) {
            System.out.print(value + " ");
        }
        System.out.

с++
#include <iostream>
#include <vector>
using namespace std;

// Функция для обмена элементов
void swap(int& a, int& b) {
    int temp = a;
    a = b;
    b = temp;
}

// Метод для разделения массива и выбора опорного элемента
int partition(vector<int>& arr, int low, int high) {
    // Выбираем последний элемент как опорный
    int pivot = arr[high];
    int i = low - 1; // Индекс меньшего элемента

    for (int j = low; j < high; j++) {
        // Если текущий элемент меньше или равен опорному
        if (arr[j] <= pivot) {
            i++;
            swap(arr[i], arr[j]);
        }
    }

    // Перемещаем опорный элемент на правильное место
    swap(arr[i + 1], arr[high]);
    return i + 1;
}

// Основной метод сортировки
void quickSort(vector<int>& arr, int low, int high) {
    if (low < high) {
        // Получаем индекс опорного элемента после partition
        int pivotIndex = partition(arr, low, high);

        // Рекурсивно сортируем левую и правую части
        quickSort(arr, low, pivotIndex - 1);
        quickSort(arr, pivotIndex + 1, high);
    }
}

// Функция для вывода массива
void printArray(const vector<int>& arr) {
    for (int value : arr) {
        cout << value << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    vector<int> array = {10, 7, 8, 9, 1, 5};
    int n = array.size();

    cout << "Исходный массив:" << endl;
    printArray(array);

    quickSort(array, 0, n - 1);

    cout << "Отсортированный массив:" << endl;
    printArray(array);

    return 0;
}

7) пИРАМИДАЛЬНАЯ СОРТИРОВКА
public class HeapSort {

    // Основной метод сортировки
    public static void heapSort(int[] arr) {
        int n = arr.length;

        // Строим max-heap (перестраиваем массив)
        for (int i = n / 2 - 1; i >= 0; i--) {
            heapify(arr, n, i);
        }

        // Извлекаем элементы из кучи поочерёдно
        for (int i = n - 1; i > 0; i--) {
            // Перемещаем текущий корень в конец
            int temp = arr[0];
            arr[0] = arr[i];
            arr[i] = temp;

            // Вызываем heapify для уменьшенной кучи
            heapify(arr, i, 0);
        }
    }

    // Преобразуем поддерево с корнем i в max-heap
    private static void heapify(int[] arr, int n, int i) {
        int largest = i;        // Инициализируем наибольший как корень
        int left = 2 * i + 1;   // Левый потомок
        int right = 2 * i + 2;  // Правый потомок

        // Если левый потомок больше корня
        if (left < n && arr[left] > arr[largest]) {
            largest = left;
        }

        // Если правый потомок больше текущего наибольшего
        if (right < n && arr[right] > arr[largest]) {
            largest = right;
        }

        // Если наибольший не корень, меняем и продолжаем heapify
        if (largest != i) {
            int swap = arr[i];
            arr[i] = arr[largest];
            arr[largest] = swap;

            // Рекурсивно применяем heapify к поддереву
            heapify(arr, n, largest);
        }
    }

    // Вспомогательный метод для вывода массива
    public static void printArray(int[] arr) {
        for (int value : arr) {
            System.out.print(value + " ");
        }
        System.out.println();
    }

    // Пример использования
    public static void main(String[] args) {
        int[] arr = {12, 11, 13, 5, 6, 7};
        System.out.println("Исходный массив:");
        printArray(arr);

        heapSort(arr);

        System.out.println("Отсортированный массив:");
        printArray(arr);
    }
}

С++
#include <iostream>
#include <vector>

using namespace std;

// Преобразуем поддерево с корнем i в max-heap
void heapify(vector<int>& arr, int n, int i) {
    int largest = i;        // Инициализируем наибольший как корень
    int left = 2 * i + 1;   // Левый потомок
    int right = 2 * i + 2;  // Правый потомок

    // Если левый потомок больше корня
    if (left < n && arr[left] > arr[largest]) {
        largest = left;
    }

    // Если правый потомок больше текущего наибольшего
    if (right < n && arr[right] > arr[largest]) {
        largest = right;
    }

    // Если наибольший не корень, меняем и продолжаем heapify
    if (largest != i) {
        swap(arr[i], arr[largest]);

        // Рекурсивно применяем heapify к поддереву
        heapify(arr, n, largest);
    }
}

// Основной метод сортировки
void heapSort(vector<int>& arr) {
    int n = arr.size();

    // Строим max-heap (перестраиваем массив)
    for (int i = n / 2 - 1; i >= 0; i--) {
        heapify(arr, n, i);
    }

    // Извлекаем элементы из кучи поочерёдно
    for (int i = n - 1; i > 0; i--) {
        // Перемещаем текущий корень в конец
        swap(arr[0], arr[i]);

        // Вызываем heapify для уменьшенной кучи
        heapify(arr, i, 0);
    }
}

// Вспомогательный метод для вывода массива
void printArray(const vector<int>& arr) {
    for (int value : arr) {
        cout << value << " ";
    }
    cout << endl;
}

// Пример использования
int main() {
    vector<int> arr = {12, 11, 13, 5, 6, 7};
    cout << "Исходный массив:" << endl;
    printArray(arr);

    heapSort(arr);

    cout << "Отсортированный массив:" << endl;
    printArray(arr);

    return 0;
}
8) Последовательный поиск
public class LinearSearch {
    /**
     * Последовательный поиск элемента в массиве.
     * @param arr массив для поиска
     * @param target искомое значение
     * @return индекс элемента, если найден; -1, если не найден
     */
    public static int linearSearch(int[] arr, int target) {
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == target) {
                return i;  // Возвращаем индекс найденного элемента
            }
        }
        return -1;  // Элемент не найден
    }

    public static void main(String[] args) {
        int[] numbers = {10, 5, 8, 3, 7, 1};
        int target = 7;
        
        int result = linearSearch(numbers, target);
        
        if (result != -1) {
            System.out.println("Элемент найден на позиции: " + result);
        } else {
            System.out.println("Элемент не найден");
        }
    }
}

С++
#include <iostream>
#include <vector>

/**
 * Последовательный поиск элемента в векторе.
 * @param arr вектор для поиска
 * @param target искомое значение
 * @return индекс элемента, если найден; -1, если не найден
 */
int linearSearch(const std::vector<int>& arr, int target) {
    for (size_t i = 0; i < arr.size(); i++) {
        if (arr[i] == target) {
            return static_cast<int>(i);  // Возвращаем индекс найденного элемента
        }
    }
    return -1;  // Элемент не найден
}

int main() {
    std::vector<int> numbers = {10, 5, 8, 3, 7, 1};
    int target = 7;
    
    int result = linearSearch(numbers, target);
    
    if (result != -1) {
        std::cout << "Элемент найден на позиции: " << result << std::endl;
    } else {
        std::cout << "Элемент не найден" << std::endl;
    }
    
    return 0;
}

9)Бинарный поиск
public class BinarySearch {

    /**
     * Бинарный поиск в отсортированном массиве.
     * @param arr отсортированный массив целых чисел
     * @param target искомое значение
     * @return индекс элемента, если найден; -1, если не найден
     */
    public static int binarySearch(int[] arr, int target) {
        if (arr == null || arr.length == 0) {
            return -1;
        }

        int left = 0;
        int right = arr.length - 1;

        while (left <= right) {
            // Избегаем переполнения при больших индексах
            int mid = left + (right - left) / 2;

            if (arr[mid] == target) {
                return mid; // Элемент найден
            } else if (arr[mid] < target) {
                left = mid + 1; // Ищем в правой половине
            } else {
                right = mid - 1; // Ищем в левой половине
            }
        }

        return -1; // Элемент не найден
    }

    // Пример использования
    public static void main(String[] args) {
        int[] sortedArray = {1, 3, 5, 7, 9, 11, 13};
        int target = 7;
        int result = binarySearch(sortedArray, target);

        if (result != -1) {
            System.out.println("Элемент найден на позиции: " + result);
        } else {
            System.out.println("Элемент не найден");
        }
    }
}

С++
#include <iostream>
#include <vector>

/**
 * Бинарный поиск в отсортированном векторе.
 * @param arr отсортированный вектор целых чисел
 * @param target искомое значение
 * @return индекс элемента, если найден; -1, если не найден
 */
int binarySearch(const std::vector<int>& arr, int target) {
    if (arr.empty()) {
        return -1;
    }

    int left = 0;
    int right = arr.size() - 1;

    while (left <= right) {
        // Избегаем переполнения при больших индексах
        int mid = left + (right - left) / 2;

        if (arr[mid] == target) {
            return mid; // Элемент найден
        } else if (arr[mid] < target) {
            left = mid + 1; // Ищем в правой половине
        } else {
            right = mid - 1; // Ищем в левой половине
        }
    }

    return -1; // Элемент не найден
}

// Пример использования
int main() {
    std::vector<int> sortedArray = {1, 3, 5, 7, 9, 11, 13};
    int target = 7;
    int result = binarySearch(sortedArray, target);

    if (result != -1) {
        std::cout << "Элемент найден на позиции: " << result << std::endl;
    } else {
        std::cout << "Элемент не найден" << std::endl;
    }

    return 0;
}
10) Интерполирующий поиск
public class InterpolationSearch {
    
    /**
     * Выполняет интерполирующий поиск в отсортированном массиве.
     * @param arr отсортированный массив целых чисел
     * @param x искомое значение
     * @return индекс найденного элемента или -1, если элемент не найден
     */
    public static int interpolationSearch(int[] arr, int x) {
        int lo = 0;
        int hi = arr.length - 1;
        
        // Пока интервал валиден и искомое значение находится в диапазоне
        while (lo <= hi && x >= arr[lo] && x <= arr[hi]) {
            
            // Если интервал сократился до одного элемента
            if (lo == hi) {
                if (arr[lo] == x) {
                    return lo;
                }
                return -1;
            }
            
            // Вычисляем предполагаемую позицию
            int pos = lo + ((hi - lo) / (arr[hi] - arr[lo])) * (x - arr[lo]);
            
            // Проверяем элемент на вычисленной позиции
            if (arr[pos] == x) {
                return pos;
            }
            
            // Если элемент больше искомого, ищем слева
            if (arr[pos] > x) {
                hi = pos - 1;
            } 
            // Если элемент меньше искомого, ищем справа
            else {
                lo = pos + 1;
            }
        }
        
        return -1; // Элемент не найден
    }
    
    // Пример использования
    public static void main(String[] args) {
        int[] arr = {10, 12, 13, 16, 18, 19, 20, 21, 22, 23, 24, 33, 35, 42, 47};
        int x = 18;
        
        int index = interpolationSearch(arr, x);
        
        if (index != -1) {
            System.out.println("Элемент найден по индексу: " + index);
        } else {
            System.out.println("Элемент не найден");
        }
    }
}

С++
#include <iostream>
using namespace std;

/**
 * Выполняет интерполирующий поиск в отсортированном массиве.
 * @param arr массив целых чисел
 * @param n размер массива
 * @param x искомое значение
 * @return индекс найденного элемента или -1, если элемент не найден
 */
int interpolationSearch(int arr[], int n, int x) {
    int lo = 0;
    int hi = n - 1;
    
    // Пока интервал валиден и искомое значение находится в диапазоне
    while (lo <= hi && x >= arr[lo] && x <= arr[hi]) {
        
        // Если интервал сократился до одного элемента
        if (lo == hi) {
            if (arr[lo] == x) {
                return lo;
            }
            return -1;
        }
        
        // Вычисляем предполагаемую позицию (используем double для точности)
        int pos = lo + (static_cast<double>(hi - lo) / (arr[hi] - arr[lo])) * (x - arr[lo]);
        
        // Проверяем элемент на вычисленной позиции
        if (arr[pos] == x) {
            return pos;
        }
        
        // Если элемент больше искомого, ищем слева
        if (arr[pos] > x) {
            hi = pos - 1;
        } 
        // Если элемент меньше искомого, ищем справа
        else {
            lo = pos + 1;
        }
    }
    
    return -1; // Элемент не найден
}

// Пример использования
int main() {
    int arr[] = {10, 12, 13, 16, 18, 19, 20, 21, 22, 23, 24, 33, 35, 42, 47};
    int n = sizeof(arr) / sizeof(arr[0]);
    int x = 18;
    
    int index = interpolationSearch(arr, n, x);
    
    if (index != -1) {
        cout << "Элемент найден по индексу: " << index << endl;
    } else {
        cout << "Элемент не найден" << endl;
    }
    
    return 0;
}

11)  Фибоначчи поиск
public class Fibonacci {
    public static int fibonacci(int n) {
        if (n <= 1) {
            return n;
        }
        return fibonacci(n - 1) + fibonacci(n - 2);
    }

    public static void main(String[] args) {
        int n = 10;
        System.out.println("Fibonacci(" + n + ") = " + fibonacci(n));
    }
}
с++
#include <iostream>
using namespace std;

int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int n = 10;
    cout << "Fibonacci(" << n << ") = " << fibonacci(n) << endl;
    return 0;
}
